<div class="uk-overflow-auto">
        <table class="uk-table uk-table-small uk-table-divider" id="myTable">
            <thead>
                <tr>
                    <th onclick="sortAccount()">Account</th>
                    <th>Box number</th>
                    <th>Item Description</th>
                    <th>Box</th>
                    <th>of Box</th>
                    <th>Qty per Box</th>
                    <th>SKU</th>
                    <th>Volume</th>
                    <th onclick="sortDate()">Date</th>
                    <th>Tracking</th>
                </tr>
            </thead>
            <tbody>

{{#each boxes as |box|}}

{{> box-data box }}

{{else}}
<h3 class="text-center">No post was found! Create one for us!</h3>
{{/each}}

            </tbody>
        </table>
    </div>

<script src="javascript/client.js"></script>
