{{#if admin}}
  <p class="uk-heading-line uk-text-right uk-margin-small-right"><span>logged in
      as
      <span
        class="uk-text-bold uk-label uk-label-success"
      >{{name}}</span></span></p>
  <form class="container">
    <div class="row justify-content-center text-center">
      <div
        class="card border-dark shadow mb-3 col-lg-4 col-sm-6 justify-content-center"
      >
        <div class="card-header">Confirmation: <span id="fba"></span></div>
        <div class="card-body text-dark">
          <h5>Notes:</h5>
          <p id="notes"></p>
          <input
            type="text"
            class="form-control"
            id="scanned_item"
            placeholder="Scan barcode to confirm"
            aria-label="Username"
            aria-describedby="basic-addon1"
            onkeyup="delay(pre_check)"
          />
          <div class="mt-3">
            <small class="card-title">Requested Boxes: Location</small>
            <ol id="inserted_item">
            </ol>
          </div>
        </div>
        <a
          href="/admin_move_main"
          class="btn btn-sm btn-primary mb-2"
        >Complete</a>
      </div>
    </div>
  </form>
  <script src="javascript/admin_pre_ship.js"></script>
{{else}}
  <script src="javascript/client_redirect.js"></script>
{{/if}}
