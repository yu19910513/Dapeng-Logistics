<div class="container text-center" id="barcode_btn">
    <h2>Account: <u>{{account}}</u></h2>
    <p>Order was placed on {{date}}</p>
    <div class="row justify-content-center"> <button type="button" class="btn btn-primary col-1" onclick="done()">Done</button></div>
</div>


{{#each boxes as |box|}}
<div class="container">
{{> barcode-data box }}
</div>

{{else}}
<script src="javascript/admin_redirect.js"></script>
<h3 class="text-center">NO ORDER WAS FOUND!</h3>
{{/each}}


<script>
    function done() {
        window.location.replace('/')
    }
</script>

{{#if admin}}
<script src="javascript/admin_hom.js"></script>
{{else}}
<script src="javascript/home.js"></script>
{{/if}}
