<div class="container text-center" id="barcode_btn">
    <h2>{{user}} - <u>{{account}}</u></h2>
    <div class="row justify-content-center">
        <button type="button" id="done" class="btn btn-sm btn-outline-primary col-1" onclick="done()">Done</button>
        <button type="button" id='go_back' class="btn btn-secondary ml-2 col-2" onclick="history.back()" style="display: none;">Go Back</button>
    </div>
</div>


{{#each containers as |container|}}
<div class="container">
{{> barcode-amazon container }}
</div>

{{else}}
<script src="javascript/admin_redirect.js"></script>
<h3 class="text-center">NO ORDER WAS FOUND!</h3>
{{/each}}


<script>
    function done() {
        window.location.replace('/')
    };
    window.onload = function() { window.print(); }
</script>
