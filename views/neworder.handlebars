<div class="container">
        <div class="row justify-content-md-center">

    <form class="col-6">
                <div class="form-group">
                    <label for="asn">ASN</label>
                    <input type="text" class="form-control" id="new_asn" value="" disabled>
                </div>

                <div class="form-group">
                    <label for="account">Account</label>
                    <input type="text" class="form-control" id="new_account" value="" placeholder="create a new account" disabled>
                </div>
                <div class="form-group col-6">
                    <label for="prefix">Prefix</label>
                    <input type="text" class="form-control" id="new_prefix" value="" placeholder="up to 3-letter prefix" disabled>
                </div>
                <div class="form-group">
                    <label for="description">Description</label>
                    <input type="text" class="form-control" id="new_des" placeholder="">
                </div>

                <div class="row">
                    <div class="col">
                    <label for="length">Length</label>
                      <input type="number" class="form-control" id="new_len" placeholder="">
                    </div>
                    <div class="col">
                        <label for="width">Width</label>
                      <input type="number" class="form-control" id="new_wid" placeholder="">
                    </div>
                    <div class="col">
                        <label for="height">Height</label>
                        <input type="number" class="form-control" id="new_hei" placeholder="">
                    </div>
                     <div class="col">
                        <label for="weight">Weight</label>
                        <input type="number" class="form-control" id="new_wei" placeholder="">
                    </div>
                </div>

                <div class="form-group">
                    <label for="total">Total Box 箱总数</label>
                    <input type="number" class="form-control" id="new_tot" placeholder="">
                </div>

                <button type="submit" id="order_pre-check" class="btn btn-primary mt-2"><a class="text-white" href="#">Submit</a></button>
              </form>
      </div>
      </div>

<script src="javascript/create.js"></script>
