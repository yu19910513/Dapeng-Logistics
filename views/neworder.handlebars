<div class="container">
        <div class="row justify-content-md-center">

    <form class="col-6">
                <div class="form-group">
                    <label for="asn">ASN</label>
                    <input type="text" class="form-control" id="new_asn" value="" disabled>
                </div>

                <div class="form-group">
                    <label for="account">Account</label>
                    <input type="text" class="form-control" id="new_account" value="" placeholder="create a new account" onkeyup="masterCheck()" disabled>
                </div>
                <div class="form-group col-6">
                    <label for="prefix">Prefix</label>
                    <input type="text" class="form-control" id="new_prefix" value="" placeholder="up to 3-letter prefix" onkeyup="prefix_check()" disabled>
                </div>
                <div class="form-group">
                    <label for="description">Description</label>
                    <input type="text" class="form-control" id="new_des" onkeyup="masterCheck()">
                </div>

                <div class="form-group">
                    <label for="sku">SKU</label>
                    <input type="text" class="form-control" id="new_sku" onkeyup="masterCheck()">
                </div>

                <div class="row">
                    <div class="col">
                    <label for="length">Length</label>
                      <input type="number" class="form-control" id="new_len" onkeyup="masterCheck()">
                    </div>
                    <div class="col">
                        <label for="width">Width</label>
                      <input type="number" class="form-control" id="new_wid" onkeyup="masterCheck()">
                    </div>
                    <div class="col">
                        <label for="height">Height</label>
                        <input type="number" class="form-control" id="new_hei" onkeyup="masterCheck()">
                    </div>
                     <div class="col">
                        <label for="weight">Weight</label>
                        <input type="number" class="form-control" id="new_wei" onkeyup="masterCheck()">
                    </div>
                </div>

                <div class="form-group">
                    <label for="total">Total Box 箱总数</label>
                    <input type="number" class="form-control" id="new_tot" onkeyup="masterCheck()">
                </div>

                <div class="form-group">
                    <label for="qty_per_box">Quantity per Box</label>
                    <input type="number" class="form-control" id="new_qty" onkeyup="masterCheck()">
                </div>

                <a id="order_pre-check" class="btn btn-primary mt-2 text-white" href="#precheck-selection" uk-toggle style="display: none;">Pre-check</a>
                <a id="fake" class="btn btn-secondary mt-2 text-white" onclick="infoAlert()">Pre-check</a>
                <div id="precheck-selection" class="uk-modal-container text-center" uk-modal>
                <div class="uk-modal-dialog">
                     <button class="uk-modal-close-default" type="button" onclick="reset()" uk-close></button>
                 <div class="uk-modal-header">
                    <h2 class="uk-modal-title">Pre-check table</h2>
                 </div>
                    <div id="fixedInfo"></div>
                <div class="uk-modal-body" oncopy="return false" oncut="return false" onpaste="return false">
                    <div id="table" class="table-editable">
                        <table class="table" id="ordertable">
                            <tr>
                                <th>Box Number</th>
                                <th>Item Description</th>
                                <th>SKU</th>
                                <th>Quantity per Box</th>
                                <th>Box</th>
                                <th>of Boxes</th>
                                <th>Weight(kg)</th>
                                <th>Length(cm)</th>
                                <th>Width(cm)</th>
                                <th>Height(cm)</th>
                            </tr>

                        </table>
                    </div>
                </div>

                <div class="uk-modal-footer uk-text-right">
                    <button class="btn btn-secondary mt-2 uk-modal-close" onclick="reset()">Cancel</button>

                    <button class="btn btn-primary mt-2 text-white" id="export-btn" onclick="exportData()" disabled>Export Data</button>
                    {{!-- <div id="modal-full" class="uk-modal-full" uk-modal>
                        <div class="uk-modal-dialog">
                            <button class="uk-modal-close-full uk-close-large" type="button" uk-close></button>
                            <div class="uk-flex uk-flex-center">
                                        <div class="uk-padding-small">
                                            <div class="barcode">

                                            </div>
                                        </div>
                                                <div class="uk-modal-footer uk-text-right">
                                    <button class="uk-button uk-button-default uk-modal-close" onclick="done()">Done</button>
                                    </div>
                            </div>
                        </div>
                    </div> --}}
                </div>
                </div>
                </div>
              </form>
      </div>
      </div>
<script src="javascript/create.js" defer></script>
<script src="javascript/home.js"></script>
