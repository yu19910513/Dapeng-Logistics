<div class="container">
        <div class="row justify-content-md-center">

    <form class="col-6">
                <div class="form-group">
                    <label for="asn">ASN</label>
                    <input type="text" class="form-control" id="new_asn" value="" disabled>
                </div>

                <div class="form-group">
                    <label for="account">Account 新帐户</label>
                    <input type="text" class="form-control" id="new_account" value="" placeholder="新帐户名 (只限英文字母)" onkeyup="masterCheck()" disabled>
                </div>
                <div class="form-group col-6">
                    <label for="prefix">Prefix 单位缩写</label>
                    <input type="text" class="form-control" id="new_prefix" value="" placeholder="限2-3英文字母" onkeyup="prefix_check()" disabled>
                </div>
                <div class="form-group">
                    <label for="description">Description 货品描述</label>
                    <input type="text" class="form-control" id="new_des" onkeyup="masterCheck()">
                </div>

                <div class="form-group">
                    <label for="sku">SKU</label>
                    <input type="text" class="form-control" id="new_sku" onkeyup="masterCheck()">
                </div>

                <div class="row">
                    <div class="col">
                    <label for="length">Length 长</label>
                      <input type="number" class="form-control" id="new_len" onkeyup="masterCheck()" placeholder="公分">
                    </div>
                    <div class="col">
                        <label for="width">Width 宽</label>
                      <input type="number" class="form-control" id="new_wid" onkeyup="masterCheck()" placeholder="公分">
                    </div>
                    <div class="col">
                        <label for="height">Height 高</label>
                        <input type="number" class="form-control" id="new_hei" onkeyup="masterCheck()" placeholder="公分">
                    </div>
                     <div class="col">
                        <label for="weight">Weight 毛重</label>
                        <input type="number" class="form-control" id="new_wei" onkeyup="masterCheck()"placeholder="公斤">
                    </div>
                </div>

                <div class="form-group">
                    <label for="total">Total Box 箱总数</label>
                    <input type="number" class="form-control" id="new_tot" onkeyup="masterCheck()">
                </div>

                <div class="form-group">
                    <label for="qty_per_box">Quantity per Box 单箱货量</label>
                    <input type="number" class="form-control" id="new_qty" onkeyup="masterCheck()">
                </div>

                <a id="order_pre-check" class="btn btn-primary mt-2 text-white" href="#precheck-selection" uk-toggle style="display: none;">Pre-check 预先校验</a>
                <a id="fake" class="btn btn-secondary mt-2 text-white" onclick="infoAlert()">Pre-check 预先校验</a>
                <div id="precheck-selection" class="uk-modal-container text-center" uk-modal>
                <div class="uk-modal-dialog">
                     <button class="uk-modal-close-default" type="button" onclick="reset()" uk-close></button>
                 <div class="uk-modal-header">
                    <h2 class="uk-modal-title">Pre-check table</h2>
                 </div>
                    <div id="fixedInfo"></div>
                <div class="uk-modal-body" oncopy="return false" oncut="return false" onpaste="return false">
                    <div id="table" class="table-editable">
                        <table class="table" id="ordertable">
                            <tr>
                                <th>Box Number<br>箱码</th>
                                <th>Item Description<br>细目</th>
                                <th>SKU<br>存货单位</th>
                                <th>Quantity per Box<br>单箱货量</th>
                                <th>Box</th>
                                <th>of Boxes<br>箱总数</th>
                                <th>Weight(kg)<br>重</th>
                                <th>Length(cm)<br>长</th>
                                <th>Width(cm)<br>宽</th>
                                <th>Height(cm)<br>高</th>
                            </tr>

                        </table>
                    </div>
                </div>

                <div class="uk-modal-footer uk-text-right">
                    <button class="btn btn-secondary mt-2 uk-modal-close" onclick="reset()">Cancel</button>

                    <button class="btn btn-primary mt-2 text-white" id="export-btn" onclick="exportData()" disabled>Export Data</button>
                    {{!-- <div id="modal-full" class="uk-modal-full" uk-modal>
                        <div class="uk-modal-dialog">
                            <button class="uk-modal-close-full uk-close-large" type="button" uk-close></button>
                            <div class="uk-flex uk-flex-center">
                                        <div class="uk-padding-small">
                                            <div class="barcode">

                                            </div>
                                        </div>
                                                <div class="uk-modal-footer uk-text-right">
                                    <button class="uk-button uk-button-default uk-modal-close" onclick="done()">Done</button>
                                    </div>
                            </div>
                        </div>
                    </div> --}}
                </div>
                </div>
                </div>
    </form>
      </div>
      </div>
<script src="javascript/create.js"></script>
