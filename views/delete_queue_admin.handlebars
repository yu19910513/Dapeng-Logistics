{{#if admin}}
<p class="uk-heading-line uk-text-right uk-margin-small-right"><span>logged in as <span class="uk-text-bold uk-label uk-label-success">{{name}}</span></span></p>
<div class="container mt-2 mb-2">
<table class="table table-hover p-2">
  <thead class="bg-warning">
    <tr>
      <th scope="col">Requestor</th>
      <th scope="col">Ref. #</th>
      <th scope="col">Targets</th>
      <th scope="col">Notes</th>
    </tr>
  </thead>
  <tbody>
    {{#each xc_boxes as |xc|}}
        {{> delete_req xc }}
    {{/each}}
    {{#each xc_containers as |xc|}}
        {{> delete_req xc }}
    {{/each}}
  </tbody>
</table>
</div>
<script src="javascript/delete_queue_admin.js"></script>
{{else}}
<script src="javascript/client_redirect.js"></script>
{{/if}}
