{{#if boxes}}
<a class="btn btn-outline-primary mt-1 ml-1 shadow-sm btn-sm" href="/account/{{id}}" onclick="accountStore({{id}})">{{name}} <span class="uk-badge">{{boxes.length}}</span></a>
{{else}}
<a class="btn btn-outline-danger mt-1 ml-1 shadow-sm btn-sm" href="/amazon/{{id}}" onclick="accountStore({{id}})">{{name}} <span class="uk-badge">{{containers.length}}</span></a>
{{/if}}
